[
  {
    "name": "${taskname}",
    "image": "${image}",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "${awslogs_region}",
        "awslogs-stream-prefix": "date",
        "awslogs-group": "${awslogs_group}"
      }
    },
    "command" : ${command}
  }
]
